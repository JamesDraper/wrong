{
    "autoload": {
        "psr-4": {
            "Wrong\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "require": {
        "psr/http-message": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^13.0",
        "mockery/mockery": "^1.6",
        "dg/bypass-finals": "^1.9",
        "phpstan/phpstan": "^2.1",
        "squizlabs/php_codesniffer": "^4.0"
    },
    "scripts": {
        "test": [
            "@phpunit",
            "@phpstan",
            "@phpcs"
        ],
        "phpunit": [
            "@phpunit.unit",
            "@phpunit.integration"
        ],
        "phpunit.integration": "phpunit --testsuite=integration",
        "phpunit.unit": "phpunit --testsuite=unit",
        "phpstan": "phpstan",
        "phpcs": [
            "@phpcs.src",
            "@phpcs.tests"
        ],
        "phpcs.src": "phpcs --standard=phpcs.src.xml src",
        "phpcs.tests": "phpcs --standard=phpcs.tests.xml tests"
    }
}
